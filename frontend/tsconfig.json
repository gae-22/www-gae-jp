{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "ignoreDeprecations": "5.0",
        "paths": {
            "~/*": ["src/*"],
            "@backend/*": ["../backend/src/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    },
    "references": [
        {
            "path": "../backend"
        }
    ],
    "include": [".astro/types.d.ts", "**/*"],
    "exclude": ["dist"]
}
