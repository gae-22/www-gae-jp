{
    "name": "www-gae-jp",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev:frontend": "cd frontend && pnpm dev",
        "dev:backend": "cd backend && pnpm dev",
        "dev": "concurrently \"pnpm dev:backend\" \"pnpm dev:frontend\"",
        "build:frontend": "cd frontend && pnpm build",
        "build:backend": "cd backend && pnpm build",
        "build": "pnpm build:backend && pnpm build:frontend",
        "start:frontend": "cd frontend && pnpm preview",
        "start:backend": "cd backend && pnpm start",
        "start": "concurrently \"pnpm start:backend\" \"pnpm start:frontend\"",
        "preview": "cd frontend && pnpm preview"
    },
    "dependencies": {},
    "devDependencies": {
        "@node-rs/argon2": "^2.0.2",
        "@shikijs/transformers": "^3.22.0",
        "@types/better-sqlite3": "^7.6.12",
        "concurrently": "^9.1.1",
        "drizzle-kit": "^0.28.0",
        "tsx": "^4.21.0",
        "yaml": "^2.8.2"
    }
}
